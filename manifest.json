{ "name": "JS Omnibox",
  "description": "The fastest way to run Javascript/jQuery on a webpage.",
  "version": "1.1",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "js"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
          "tabs", "http://*/*", "https://*/*"
        ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  }
}
